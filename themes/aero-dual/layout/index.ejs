<%- partial('_partial/header', {headerType: 'index'}) %>
<!-- Home Page Post List -->
<div class="container">
  <div class="row">
    <div class="col-md-9">
      <% page.posts.each(function(post){ %>
        <%- partial('_partial/index-entry', {post: post, index: true}) %>
      <% }) %>
    </div>

    <% if (theme.widgets.length) { %>
        <%- partial('_partial/sidebar') %>
    <% } %>
    </div>
  </div>

<%- partial('_partial/pagination') %>
