<%- partial('_partial/header', {headerType: 'index'}) %>
<!-- Home Page Post List -->
<div class="container">
  <div class="row">
    <div class="col-md-9">
      <% page.posts.each(function(post){
        var if_add = true
        post.categories.forEach(function(category){
          if (category.name == "nonsense") {
            if_add = false
          }
        }) %>
        <% if (if_add) { %>
          <%- partial('_partial/index-entry', {post: post, index: true}) %>
        <% } %>
      <% }) %>
    </div>

    <% if (theme.widgets.length) { %>
        <%- partial('_partial/sidebar') %>
    <% } %>
    </div>
  </div>

<%- partial('_partial/pagination') %>
